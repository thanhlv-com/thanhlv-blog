  "name": "blog",
  "type": "module",
  "scripts": {
    "prebuild:dev": "node script/blogs.js env:dev",
    "prebuild:live": "node script/blogs.js env:live",
    "dev": " yarn run prebuild:dev && vitepress",
    "build": "yarn run prebuild:live && vitepress build",
    "build_github": "yarn run prebuild:live && vitepress build --outDir=./../../docs",
