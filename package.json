{
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "scripts": {
    "pre_start": "node prebuild/prebuild.cjs",
    "pre_run": "yarn run pre_start",
    "dev": "yarn run pre_run && vitepress",
    "build": "yarn run pre_run && vitepress build",
    "preview": "vitepress preview",
    "preinstall": "npx only-allow pnpm",
    "ci": "npm i -g pnpm && pnpm install",
    "pdf-blog": "press-export-pdf export src/blog"
  },
  "dependencies": {
    "@vue/repl": "^3.0.0",
    "@vue/theme": "^2.2.5",
    "dynamics.js": "^1.1.5",
    "gsap": "^3.9.0",
    "vitepress": "1.0.0-rc.33",
    "vitepress-export-pdf": "1.0.0-beta.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^20.10.1",
    "terser": "^5.14.2"
  },
  "pnpm": {
    "overrides": {
      "@vitejs/plugin-vue": "5.0.0-beta.1"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react",
        "search-insights"
      ]
    }
  }
}
